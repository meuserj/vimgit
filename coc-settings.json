{
    "git.realtimeGutters": false,
    "eslint.run": "onSave",
    "eslint.alwaysShowStatus": true,
    "eslint.probe": [
        "javascript",
        "pug",
        "typescript",
        "html",
        "markdown"
    ],
    "markdownlint.config": {
        "first-line-h1": false,
        "line-length": false,
        "no-duplicate-header": false
    }
}
